{
  "autoDeploy": true,
  "hotAlignment": true,
  "executionMode": "synchronous",
  "readQuorum": 1,
  "writeQuorum": "majority",
  "failureAvailableNodesLessQuorum": false,
  "readYourWrites": true,
  "servers": {
    "*": "master"
  },
  "clusters": {
    "internal": {
      "servers": ["*"]
    },
    "index": {
      "servers": ["*"]
    },
    "*": {
      "servers": ["orientdb-node1", "orientdb-node2", "orientdb-node3", "orientdb-node4"]
    }
  }
}
